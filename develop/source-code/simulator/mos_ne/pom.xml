            <artifactId>uep_server</artifactId>
        </dependency>
        <dependency>
            <groupId>com.zte.uep</groupId>
            <artifactId>uep_server_3rd</artifactId>
        </dependency>
        <dependency>
            <groupId>com.zte.mos.app</groupId>
            <artifactId>app-vm-ne</artifactId>
        </dependency>
        <dependency>
            <groupId>com.zte.mos.app</groupId>
            <artifactId>app-pm-ne</artifactId>
        </dependency>
        <dependency>
